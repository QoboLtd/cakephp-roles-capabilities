{
    "name": "qobo/cakephp-roles-capabilities",
    "description": "Roles and Capabilities plugin for CakePHP",
    "type": "cakephp-plugin",
	"license": "MIT",
	"authors": [
		{
			"name": "Qobo Ltd",
			"email": "info@qobo.biz",
			"homepage": "https://www.qobo.biz",
			"role": "Developer"
		}
	],
    "require": {
        "qobo/cakephp-utils": "^9.0",
        "qobo/cakephp-groups": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "cakephp/cakephp-codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Qobo\\RolesCapabilities\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qobo\\RolesCapabilities\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
