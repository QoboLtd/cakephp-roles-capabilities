{
    "name": "qobo/cakephp-roles-capabilities",
    "description": "Roles and Capabilities plugin for CakePHP",
    "keywords": ["cakephp", "roles", "acl", "security"],
    "type": "cakephp-plugin",
    "license": "MIT",
    "homepage": "https://www.qobo.biz",
    "authors": [
        {
            "name": "Qobo Ltd",
            "email": "support@qobo.biz",
            "homepage": "https://www.qobo.biz",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/QoboLtd/cakephp-roles-capabilities/issues",
        "source": "https://github.com/QoboLtd/cakephp-roles-capabilities"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require": {
        "qobo/cakephp-utils": "^13.2.9",
        "qobo/cakephp-groups": "^11.5.1"
    },
    "require-dev": {
        "brianium/paratest": "^1.1",
        "cakephp/bake": "^1.1",
        "cakephp/cakephp-codesniffer": "^3.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-webmozart-assert": "^0.12",
        "phpunit/phpunit": "^6.0",
        "psy/psysh": "@stable",
        "slevomat/coding-standard": "^6.0",
        "thecodingmachine/phpstan-strict-rules": "^0.12",
        "timeweb/phpstan-enum": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "RolesCapabilities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RolesCapabilities\\Test\\": "tests/",
            "Groups\\Test\\": "vendor/qobo/cakephp-groups/tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "scripts": {
        "test": [
            "phpcs",
            "phpunit --no-coverage"
        ],
        "test-coverage": [
            "phpcs",
            "phpunit"
        ]
    },
    "scripts-descriptions": {
        "test": "Runs phpcs and phpunit without coverage",
        "test-coverage": "Runs phpcs and phpunit with coverage enabled"
    },
    "prefer-stable": true
}
