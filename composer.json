{
    "name": "qobo/cakephp-roles-capabilities",
    "description": "Roles and Capabilities plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "GPL-2.0",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "qobo/cakephp-groups": "^3.0",
        "muffin/trash": "^1.1",
        "maiconpinto/cakephp-adminlte-theme": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "cakephp/cakephp-codesniffer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "RolesCapabilities\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RolesCapabilities\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
